    "typescript": "~5.7.2",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.3",
    "@types/node": "^22.15.3",
    "eslint": "^9.22.0",
    "@eslint/js": "^9.22.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "tsup": "^8.4.0"
  },
  "keywords": [
    "react",
    "component-library",
    "vite",
    "typescript",
    "ui",
    "components"
