{
  "name": "@stephan/test-component-library",
  "version": "1.0.0",
  "description": "Reusable components for building web page sections",
  "main": "dist/test-component-library.cjs.js",
  "module": "dist/test-component-library.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
